    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^5.0.1",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "preact": "^8.1.0",
    "weui-sass": "github:afeiship/weui-sass"
