{
  "name": "preact-weui",
  "version": "0.0.0",
  "description": "Weui for preact.",
  "homepage": "https://github.com/afeiship/preact-weui",
  "author": {
    "name": "afei",
    "email": "1290657123@qq.com"
  },
  "scripts": {
    "clean": "rimraf dist",
    "example":"webpack-dev-server --inline --colors --config ./build/webpack.example.conf.js",
