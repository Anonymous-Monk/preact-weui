{
  "name": "preact-weui",
  "version": "0.0.1",
  "description": "Weui for preact.",
  "homepage": "https://github.com/afeiship/preact-weui",
  "author": {
    "name": "afei",
    "email": "1290657123@qq.com"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --inline --colors --config ./build/webpack.example.conf.js",
    "prebuild": "cross-env NODE_ENV=production npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.prod.conf.js",
    "predev": "cross-env NODE_ENV=development npm run dll",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --colors --config ./build/webpack.dev.conf.js",
    "publish":"npm publish"
  },
  "main": "dist/preact-weui.js",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-array-from": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "glob": "^7.1.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
